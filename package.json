{
  "name": "@alienfast/storybook-addon-form",
  "version": "2.0.8",
  "description": "Storybook addon for form information",
  "keywords": [
    "storybook-addons"
  ],
  "packageManager": "yarn@3.5.0",
  "engines": {
    "yarn": ">=2.0.0"
  },
  "repository": "https://github.com/alienfast/storybook-addon-form",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "type": "module",
  "main-note": "This is ONLY to appease tsc in moduleResolution: node see https://github.com/rosskevin/ts-esm-workspaces/tree/bug-main-required-to-build",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./manager": {
      "types": "./dist/manager.d.ts",
      "import": "./dist/manager.js",
      "require": "./dist/manager.cjs"
    },
    "./register": {
      "types": "./dist/manager.d.ts",
      "import": "./dist/manager.js",
      "require": "./dist/manager.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "clean:yarn": "rimraf yarn.lock node_modules .yarn/install-state.gz && yarn cache clean",
    "build": "yarn clean && tsup",
    "build:ide": "tsc",
    "build:watch": "tsup --watch",
    "test": "echo \"no tests\"",
    "storybook": "sb dev -p 6006",
    "start": "run-p build:watch storybook",
    "storybook:build": "storybook build -o .storybook-out",
    "release": "yarn build && yarn auto shipit",
    "lint": "eslint . --cache --ext .js,.ts,.tsx",
    "lint:fix": "eslint . --cache --ext .js,.ts,.tsx --fix"
  },
  "dependencies": {
    "react-syntax-highlighter": "^15.5.0"
  },
  "devDependencies": {
    "@alienfast/eslint-config": "^4.0.7",
    "@alienfast/tsconfig": "^1.0.1",
    "@storybook/addon-essentials": "^7.0.7",
    "@storybook/addons": "^7.0.7",
    "@storybook/api": "^7.0.7",
    "@storybook/cli": "^7.0.7",
    "@storybook/components": "^7.0.7",
    "@storybook/core-events": "^7.0.7",
    "@storybook/preview-api": "^7.0.7",
    "@storybook/react": "^7.0.7",
    "@storybook/react-vite": "^7.0.7",
    "@storybook/theming": "^7.0.7",
    "@swc/core": "^1.3.55",
    "@types/eslint": "^8",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.6",
    "@types/react-syntax-highlighter": "^15",
    "auto": "^10.46.0",
    "eslint": "^8.39.0",
    "final-form": "^4.20.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rimraf": "^5.0.0",
    "storybook": "^7.0.7",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "@storybook/addons": "^7.0.7",
    "@storybook/api": "^7.0.7",
    "@storybook/components": "^7.0.7",
    "@storybook/core-events": "^7.0.7",
    "@storybook/theming": "^7.0.7",
    "final-form": "^4.20.7",
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "resolutions": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.6"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "storybook": {
    "displayName": "Addon Form",
    "supportedFrameworks": [
      "supported-frameworks"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  },
  "author": "Kevin Ross <kevin.ross@alienfast.com>"
}
