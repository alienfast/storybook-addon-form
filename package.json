{
  "name": "@alienfast/storybook-addon-form",
  "version": "3.0.7",
  "description": "Storybook addon for form information",
  "keywords": [
    "storybook-addons"
  ],
  "packageManager": "yarn@4.9.4",
  "engines": {
    "node": ">=20.19.0",
    "yarn": ">=2.0.0"
  },
  "repository": "https://github.com/alienfast/storybook-addon-form",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./preview": {
      "types": "./dist/index.d.ts",
      "import": "./dist/preview.js"
    },
    "./preset": "./dist/preset.js",
    "./manager": "./dist/manager.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "clean:yarn": "rimraf yarn.lock node_modules .yarn/install-state.gz && yarn cache clean",
    "prebuild": "yarn clean",
    "build": "tsup",
    "typecheck": "tsc",
    "build:watch": "yarn build --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "run-p build:watch storybook",
    "release": "yarn build && auto shipit",
    "storybook": "rimraf node_modules/.cache || true && storybook dev -p 6006",
    "build-storybook": "storybook build -o .storybook-out",
    "lint": "eslint . --cache --ext .js,.ts,.tsx",
    "lint:fix": "eslint . --cache --ext .js,.ts,.tsx --fix"
  },
  "dependencies": {
    "@storybook/icons": "^1.6.0",
    "react-syntax-highlighter": "^16.1.0"
  },
  "devDependencies": {
    "@alienfast/eslint-config": "^6.0.9",
    "@alienfast/tsconfig": "^1.0.8",
    "@auto-it/all-contributors": "^11.3.0",
    "@auto-it/first-time-contributor": "^11.3.0",
    "@auto-it/released": "^11.3.0",
    "@storybook/addon-docs": "^10.0.6",
    "@storybook/react-vite": "^10.0.6",
    "@types/eslint": "^9",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/react-syntax-highlighter": "^15",
    "@vitejs/plugin-react": "^5.1.0",
    "auto": "^11.3.0",
    "eslint": "^9.39.1",
    "final-form": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^6.1.0",
    "storybook": "^10.0.6",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.2.2"
  },
  "peerDependencies": {
    "storybook": "^10.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "resolutions": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.6"
  },
  "bundler": {
    "exportEntries": [
      "src/index.ts"
    ],
    "managerEntries": [
      "src/manager.tsx"
    ],
    "previewEntries": [
      "src/preview.ts"
    ],
    "nodeEntries": [
      "src/preset.ts"
    ]
  },
  "storybook": {
    "displayName": "Addon Form",
    "supportedFrameworks": [
      "supported-frameworks"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  },
  "author": "Kevin Ross <kevin.ross@alienfast.com>",
  "auto": {
    "plugins": [
      "npm",
      "all-contributors",
      "first-time-contributor",
      "released"
    ]
  }
}
