{
  "name": "@alienfast/storybook-addon-form",
  "version": "2.0.0",
  "description": "Storybook addon for form information",
  "keywords": [
    "storybook-addons"
  ],
  "packageManager": "yarn@3.2.2",
  "engines": {
    "yarn": ">=2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alienfast/storybook-addon-form"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "module": "dist/esm/index.js",
  "types": "dist/ts/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "clean": "rimraf ./dist ./build",
    "clean:yarn": "rimraf yarn.lock node_modules .yarn/install-state.gz && yarn cache clean",
    "b": "tsc --outDir build",
    "buildBabel": "babel ./src -d ./dist/esm --env-name esm --extensions \".js,.jsx,.ts,.tsx\"",
    "buildTsc": "tsc --declaration --emitDeclarationOnly --outDir ./dist/ts",
    "prebuild": "yarn clean",
    "build": "run-p buildBabel buildTsc",
    "build:watch": "run-p \"buildBabel --watch\" \"buildTsc --watch\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "sb dev -p 6006 --no-manager-cache --debug-webpack",
    "start": "run-p build:watch storybook",
    "storybook:build": "sb build -o .storybook-out",
    "release": "yarn build && yarn auto shipit",
    "lint": "eslint . --cache --ext .js,.ts,.tsx",
    "lint:fix": "eslint . --cache --ext .js,.ts,.tsx --fix"
  },
  "dependencies": {
    "react-json-view": "^1.21.3"
  },
  "devDependencies": {
    "@alienfast/eslint-config": "^3.0.25",
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@storybook/addon-essentials": "^7.0.0-alpha.33",
    "@storybook/addons": "^7.0.0-alpha.33",
    "@storybook/api": "^7.0.0-alpha.33",
    "@storybook/cli": "^7.0.0-alpha.33",
    "@storybook/components": "^7.0.0-alpha.33",
    "@storybook/core-events": "^7.0.0-alpha.33",
    "@storybook/react": "^7.0.0-alpha.33",
    "@storybook/react-webpack5": "^7.0.0-alpha.33",
    "@storybook/theming": "^7.0.0-alpha.33",
    "@types/eslint": "^8",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.6",
    "auto": "^10.37.6",
    "babel-loader": "^8.2.5",
    "eslint": "^8.23.1",
    "final-form": "^4.20.7",
    "jest": "^29.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "peerDependencies": {
    "@storybook/addons": "^7.0.0-alpha.33",
    "@storybook/api": "^7.0.0-alpha.33",
    "@storybook/components": "^7.0.0-alpha.33",
    "@storybook/core-events": "^7.0.0-alpha.33",
    "@storybook/theming": "^7.0.0-alpha.33",
    "final-form": "^4.20.7",
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-json-view": "^1.21.3"
  },
  "resolutions": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.6"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "storybook": {
    "displayName": "Addon Form",
    "supportedFrameworks": [
      "supported-frameworks"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  }
}
