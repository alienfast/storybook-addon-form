{
  "name": "@alienfast/storybook-addon-form",
  "version": "3.0.0",
  "description": "Storybook addon for form information",
  "keywords": [
    "storybook-addons"
  ],
  "packageManager": "yarn@4.9.4",
  "engines": {
    "yarn": ">=2.0.0"
  },
  "repository": "https://github.com/alienfast/storybook-addon-form",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "type": "module",
  "main-note": "This is ONLY to appease tsc in moduleResolution: node see https://github.com/rosskevin/ts-esm-workspaces/tree/bug-main-required-to-build",
  "main": "dist/index.js",
  "xxtypes": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs"
    },
    "./manager": {
      "import": "./dist/manager.js",
      "types": "./dist/manager.d.ts",
      "require": "./dist/manager.cjs"
    },
    "./preview": {
      "import": "./dist/preview.js",
      "types": "./dist/preview.d.ts",
      "require": "./dist/preview.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "clean:yarn": "rimraf yarn.lock node_modules .yarn/install-state.gz && yarn cache clean",
    "prebuild": "yarn clean",
    "build": "tsup",
    "build:ide": "tsc",
    "build:watch": "yarn build --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "run-p build:watch storybook",
    "release": "yarn build && auto shipit",
    "storybook": "rimraf node_modules/.cache || true && storybook dev -p 6006",
    "build-storybook": "storybook build -o .storybook-out",
    "lint": "eslint . --cache --ext .js,.ts,.tsx",
    "lint:fix": "eslint . --cache --ext .js,.ts,.tsx --fix"
  },
  "dependencies": {
    "@storybook/icons": "^1.4.0",
    "react-syntax-highlighter": "^15.6.6"
  },
  "devDependencies": {
    "@alienfast/eslint-config": "^6.0.5",
    "@alienfast/tsconfig": "^1.0.4",
    "@auto-it/all-contributors": "^11.3.0",
    "@auto-it/first-time-contributor": "^11.3.0",
    "@auto-it/released": "^11.3.0",
    "@storybook/react-vite": "^9.1.4",
    "@storybook/testing-library": "^0.2.2",
    "@types/eslint": "^9",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@types/react-syntax-highlighter": "^15",
    "@vitejs/plugin-react": "^5.0.2",
    "auto": "^11.3.0",
    "eslint": "^9.34.0",
    "final-form": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rimraf": "^6.0.1",
    "storybook": "^9.1.4",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0",
    "vite": "^7.1.4"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "storybook": "^9.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "resolutions": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "@types/react": "^16.8.6",
    "@types/react-dom": "^16.8.6"
  },
  "storybook": {
    "displayName": "Addon Form",
    "supportedFrameworks": [
      "supported-frameworks"
    ],
    "icon": "https://user-images.githubusercontent.com/321738/63501763-88dbf600-c4cc-11e9-96cd-94adadc2fd72.png"
  },
  "author": "Kevin Ross <kevin.ross@alienfast.com>",
  "auto": {
    "plugins": [
      "npm",
      "all-contributors",
      "first-time-contributor",
      "released"
    ]
  }
}
